{
  "name": "beat_saber",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "VITE_APP_GIT_SHA=`git rev-parse --short HEAD` vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path ./.gitignore",
    "lint:check": "eslint ./src --ext .jsx,.js,.ts,.tsx",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "npm run lint:format && npm run lint:fix ",
    "type-check": "tsc",
    "release": "npm run type-check && npm run lint:fix && npm run lint:format && npm run build",
    "prepare": "husky install",
    "test-unit": "vitest",
    "test-unit-ui": "vitest --ui"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-resize-detector": "^7.1.2",
    "react-router-dom": "^5.2.0",
    "stats": "^1.0.0",
    "three": "^0.157.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.1.8",
    "@types/stats": "^0.16.30",
    "@types/three": "^0.156.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/ui": "^0.33.0",
    "autoprefixer": "^10.3.1",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.7.3",
    "eslint-plugin-vitest-globals": "^1.2.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^2.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.3.2",
    "vite": "^4.4.9",
    "vite-plugin-glsl": "^0.2.2",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.33.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.md": "prettier --write"
  }
}
